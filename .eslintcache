],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom';\nimport axios from 'axios'\nimport { Card, Button } from 'react-bootstrap'\n\nconst Desease = (props) => {\n\n    let params = props.match.params;\n    const [desease, setDesease] = useState([]);\n    const [symptoms, setSymptoms] = useState([]);\n\n    useEffect(() => {\n        axios.get('/deseases/'+params.id)\n        .then(\n            response => {\n                setDesease(response.data)\n            }\n        )\n\n        axios.get('/desease-symptoms/'+params.id)\n        .then(\n            response => {\n                setSymptoms(response.data)\n            }\n        )\n    }, [])\n\n    const deleteId = () => {\n        axios.delete(\"/deseases/\"+desease.id)\n          .then(res => console.log(res))\n          .catch(err => console.log(err))\n          .then(setTimeout(() =>  props.history.push(\"/deseases\"), 700 ))\n    }\n\n    return (<>\n        <Card>\n            <Card.Header>{desease.title}</Card.Header>\n            <Card.Body>\n                <Card.Title>{desease.body_type}</Card.Title>\n                <Card.Text>\n                    {desease.description}\n                </Card.Text>\n                <Link to={{pathname:'/edit-desease/' + desease.id}}>\n                    <Button variant=\"dark\">\n                        Edit\n                    </Button>\n                </Link>\n                <Button variant=\"secondary\" onClick={deleteId}>\n                    Delete\n                </Button>\n            </Card.Body>\n            <Card> \n            {symptoms.length > 0 ? \"Symptoms: \" : \"\"}\n            {symptoms.map(symptom => {\n                return (\n                    <>\n                    <Link to={{pathname:'/symptoms/' + symptom.id}} key={symptom.id}>\n                        <Card.Title>\n                            {symptom.title} \n                        </Card.Title>\n                    </Link>\n                    <Card.Text>\n                        {symptom.description}\n                    </Card.Text> \n                    </>\n                )}\n            )}\n        </Card>\n            <Card.Footer className=\"text-muted\">{desease.created_at}</Card.Footer>\n        </Card>\n        \n        </>\n    );\n};\n\nexport default Desease;","/Users/anastasia/React/src/components/Symptom.js",["69","70"],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom';\nimport axios from 'axios'\n\nconst Symptome = (props) => {\n\n    let params = props.match.params;\n    const [symptom, setSymptom] = useState([]);\n\n    useEffect(() => {\n        axios.get('/symptoms/'+params.id)\n        .then(\n            response => {\n                setSymptom(response.data)\n            }\n        )\n    }, [])\n\n    return (\n        <>\n        <div className=\"flex\">\n            {symptom.title}\n            {symptom.description}\n        </div>\n        </>\n    );\n};\n\nexport default Symptome","/Users/anastasia/React/src/components/DeseaseContainer.js",[],"/Users/anastasia/React/src/components/Home.js",[],"/Users/anastasia/React/src/components/SymptomsContainer.js",[],"/Users/anastasia/React/src/components/NotFound.js",["71"],"import React, { useState } from 'react'\n\nconst NotFound = () => {\n  return (\n    <>\n      <div className=\"flex\">\n        <h1 className=\"mt-5\">404</h1>\n        <p>Oops! Something is wrong.</p>\n        <a class=\"button\" href=\"/\"><i class=\"icon-home\"></i> Go back in initial page, is better.</a>\n      </div>\n    </>\n  );\n};\n\nexport default NotFound","/Users/anastasia/React/src/components/forms/desease.form.js",[],"/Users/anastasia/React/src/components/forms/symptoms.form.js",[],"/Users/anastasia/React/src/components/forms/edit-desease.js",["72","73"],"import React, { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport {\n    Form,\n    Button, \n    Container\n} from 'react-bootstrap'\n\nconst EditDesease = (props) => {\n\n    let params = props.match.params;\n\n    const [desease, setDesease] = useState([]);\n    const [localTitle, setLocalTitle] = useState('');\n    const [localDescription, setLocalDescription] = useState('');\n    const [localBodyPart, setLocalBodyPart] = useState('');\n\n    useEffect(() => {\n        axios.get('/deseases/'+params.id)\n        .then(\n            response => {\n                setDesease(response.data)\n                setLocalTitle(response.data.title)\n                setLocalDescription(response.data.description)\n                setLocalBodyPart(response.data.body_part)\n            }\n        )\n    }, [])\n\n    const handleTitleChange = (event) => {\n        setLocalTitle( event.target.value )\n    }\n\n    const handleDescriptionChange = (event) => {\n        setLocalDescription( event.target.value )\n    }\n\n    const handleBodyPartChange = (event) => {\n        setLocalBodyPart( event.target.value )\n    }\n\n    const handleSubmit = (event) => {\n        event.preventDefault()\n\n        const title = event.target.title.value\n        const description = event.target.description.value\n        const body_part = event.target.body_part.value\n        const data = {\n            title: title,\n            description: description,\n            body_part: body_part\n        }\n\n        axios.put(\"/deseases/\"+desease.id, data, {headers: {\n            'Content-Type': 'application/json',\n        }})\n        .then(res => console.log(res))\n        .catch(err => console.log(err))\n        .then(setTimeout(() =>  props.history.push(\"/deseases\"), 700 ))\n    }\n\n    return(\n        <Container>\n            <div class=\"mt-5 w-50 text-justify\">\n                <h1 class=\"mb-4\">Update Desease</h1>\n                <Form  onSubmit={handleSubmit}>\n\n                <Form.Group controlId=\"title\">\n                    <Form.Label>Title</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Enter title\" id=\"title\"\n                        value={localTitle}\n                        onChange={handleTitleChange} />\n                </Form.Group>\n\n                <Form.Group controlId=\"description\">\n                    <Form.Label>Description</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Description\" id=\"description\"\n                        value={localDescription}\n                        onChange={handleDescriptionChange} />\n                </Form.Group>\n\n                <Form.Group controlId=\"body_part\">\n                    <Form.Label>Body Part</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Body part\" id=\"body_part\" \n                        onChange={handleBodyPartChange} />\n                    <Form.Text className=\"text-muted\">\n                        Head, heart, arms, legs...\n                    </Form.Text>\n                </Form.Group>\n\n                <Button variant=\"primary\" size=\"lg\" type=\"submit\" block>\n                    Submit\n                </Button>\n                </Form>\n            </div>\n        </Container>\n    )}\n\nexport default EditDesease\n","/Users/anastasia/React/src/components/layout/Navigation.js",[],"/Users/anastasia/React/src/components/helpers/TimeAgo.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"74","replacedBy":"78"},{"ruleId":"76","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":26,"column":8,"nodeType":"82","endLine":26,"endColumn":10,"suggestions":"83"},{"ruleId":"84","severity":1,"message":"85","line":2,"column":10,"nodeType":"86","messageId":"87","endLine":2,"endColumn":14},{"ruleId":"80","severity":1,"message":"81","line":17,"column":8,"nodeType":"82","endLine":17,"endColumn":10,"suggestions":"88"},{"ruleId":"84","severity":1,"message":"89","line":1,"column":17,"nodeType":"86","messageId":"87","endLine":1,"endColumn":25},{"rule